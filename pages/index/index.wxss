/**index.wxss**/
.total{
	position: relative;
  overflow: hidden;
  background-image: url("data:image/jpg;base64,");
  background-size: 100% 100%;
}
.canvas {
	box-sizing: border-box;
	display: block;
}
.box-swiper{
	  margin: 40rpx auto;
    min-height: 200rpx;
    width: 600rpx;
    padding: 20rpx;
    position:relative;
    background: -webkit-gradient(linear, 0% 20%, 0% 100%, from(rgb(243, 165, 208)), to(rgb(248, 147, 223)), color-stop(.2, #f2f2f2));
    border: 2rpx solid #ccc;
    box-shadow: 0rpx 0rpx 20rpx rgba(0, 0, 0, 0.3);
		opacity: 0;
}
.title{
		font-size: 40rpx;
		font-family: '楷体';
    font-weight: bold;
    margin-top: 10rpx; 
    text-shadow: 2rpx 2rpx 6rpx rgba(243, 149, 212, 0.993);
		text-align: center;
}
.swiper{
	width: 100%;
	box-sizing: border-box;
  padding: 0 100rpx;
	border-bottom: 2rpx rgb(0, 195, 255);
}
.swiper image{
	margin-top: 30rpx;
	width: 100%;
}
.swiper-word{
	margin-top: 30rpx;
	font-size: 35rpx;
	text-align: justify;
	font-family: '楷体';
}
.container{
	height:1100rpx;
  width:100%;
	opacity: 0;
	display: flex; 
	flex-flow:row wrap;
	align-content: space-between;
}

.pro-personlogo{
	width: 384rpx;
}
.item-one,.item-two,.item-three,.item-four,.item-five,.item-six,.item-seven,.item-eight,.item-nine{
	text-align: center;
}
.item-one,.item-three,.item-four,.item-six,.item-seven,.item-nine{
	width: 20%;
	height: 280rpx;
	opacity: 0;
}
.item-two,.item-five{
	width: 60%;
	height: 280rpx;
}
.item-eight{
	width: 60%;
	height: 330rpx;
	opacity: 0;
}
.item-ten,.item-eleven,.item-twelve{
	width: 100%;
	height: 60rpx;
	text-align: center;
	font-family: '楷体';
	font-size: 35rpx;
	color: rgb(243, 106, 129);
}
.item-text1,.item-text2,.item-text3{
	font-family: '楷体';
	font-size: 35rpx;
	opacity: 0;
}
.nav,.footer{
	height: 80rpx;
	width: 100%;
	text-align: center;
	font-family: '宋体';
	font-size: 30rpx;
}

.ripple {
    background-color: rgba(72, 189, 235, 0.959);
    border-radius: 100%;
    height:10px;
    width:10px;
    margin-top: -90px;
    position: absolute;
    -webkit-transform: scale(0);
}
@-webkit-keyframes ripple {
    100% {
    -webkit-transform: scale(5);
    transform: scale(5);
    background-color: transparent;
    }
}