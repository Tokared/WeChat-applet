.container,page {
  height: 100%;
  width: 100%;
	font-family: '楷体';
}

.container{
	position: relative;
  overflow: hidden;
  background-image: url("data:image/jpg;base64,");
  background-size: 100% 100%;
}

.top {
  position: absolute;
  top: 0;
  left: 0;
}
.top .top-bg{
  width: 100%;
	height: 52%;
  opacity: 0;
}
.top .top-one,.top .top-two,.top .top-three,.music-word{
	position: absolute;
	left: 21%;
	height: 20rpx;
	width: 450rpx;
	text-align: center;
	font-weight: 700;
  color: #be6fff;
	opacity: 0;
}
.top .top-one{
  top: 36%;
  font-size: 36rpx;
}
.top .top-two{
	top: 42%;
  border-radius: 5rpx;
  padding: 2rpx;
  font-size: 45rpx;
}
.top .top-three{
	top: 50%;
  padding: 2rpx;
  font-size: 35rpx;
  color: #dc85ee;
}
.music-word{
	top: 56%;
  padding: 2rpx;
  font-size: 35rpx;
  color: #dc85ee;

}
.bottom {
  position: absolute;
}
.bottom .bottom-one,.bottom .bottom-one2,.bottom .bottom-two,.bottom .bottom-two2,.bottom .bottom-three,.bottom .bottom-three2,.bottom .bottom-four,.bottom .bottom-four2{
	position: absolute;
  opacity: 0;
  width: 100%;
}
.bottom-one,.bottom-one2{
  height: 18%;
  bottom: 10%;
}
.bottom-one2 {
  right: -100%;
}
.bottom-two,.bottom-two2{
  height: 20%;
	bottom: 5%;
}
.bottom-two2 {
	right: -100%;
}
.bottom-three,.bottom-three2{  
  height: 18%;
  bottom: 0;
}
.bottom-three2 {  
	right: -100%;
}
.bottom-four,.bottom-four2{
  height: 20%;
  bottom: 0;
}
.bottom-four2 {
	right: -100%;
}

.bottom-1s-move {
  opacity: 1;
  animation: a 4s linear infinite;
}

.bottom-2s-move {
  opacity: 1;
  animation: a 6s linear infinite;
}

.bottom-3s-move {
  opacity: 1;
  animation: a 8s linear infinite;
}

.bottom-4s-move {
  opacity: 1;
  animation: a 10s linear infinite;
}

